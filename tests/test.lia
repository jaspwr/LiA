use graphicx, [utf8]inputenc

@img = (src, desc) => {
    env center {
        \image{@("images/" + src), 10cm}
        \linebreak
        env textit {
            @desc
        }
    }
}
# Cool Cat Image
@img(cat.png, Meow 🐈!)

@muliplication = (a: Number, b: Number) => $@a \times @b = @(a * b)$
